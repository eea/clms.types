<html xmlns="http://www.w3.org/1999/xhtml" xmlns:metal="http://xml.zope.org/namespaces/metal" xmlns:tal="http://xml.zope.org/namespaces/tal" xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="dipc.addon" metal:use-macro="context/main_template/macros/master">

  <body>
    <metal:block fill-slot="content-core">
      <p>
        <tal:uid tal:content="string:Plone uuid: ${context/uuid}"></tal:uid>
      </p>
      <p>
        <tal:uid tal:content="string:Updated GeoNetwork uuids: ${view/geonetwork_ids}"></tal:uid>
      </p>
      <p>
        <tal:uid tal:repeat="data view/json_data">
          <strong tal:content="data"></strong>
          <br />
          <span tal:content="python:view.json_data[data].get('data')"></span>
          <hr />
        </tal:uid>
      </p>
    </metal:block>
  </body>
</html>